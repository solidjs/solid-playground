    "@types/workbox-sw": "4.3.1",
    "assert": "2.0.0",
    "autoprefixer": "^10.2.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.2.15",
    "tailwindcss": "^2.1.2",
    "typescript": "4.2.4",
    "vite": "2.3.0",
    "vite-plugin-solid": "^1.7.0",
    "workbox-cli": "6.1.5"
  },
  "dependencies": {
    "@amoutonbrady/lz-string": "^0.0.1",
